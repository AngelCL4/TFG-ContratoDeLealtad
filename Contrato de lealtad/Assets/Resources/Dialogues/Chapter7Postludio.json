    {
      "character": "Demain",
      "portrait": "DemainTriste",
      "position": "left",
      "facingDirection": "right",
      "text": "¿Qué?"
    },
    {
      "character": "Lisella",
      "portrait": "LisellaNeutral",
      "position": "left",
      "facingDirection": "right",
      "text": "Hey. Somos Demain y Lisella, los príncipes de Hislan, ¿no os acordais?"
