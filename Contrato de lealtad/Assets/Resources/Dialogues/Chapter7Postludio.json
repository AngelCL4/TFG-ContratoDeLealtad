    {
      "character": "Lisella",
      "portrait": "LisellaNeutral",
      "position": "left",
      "facingDirection": "right",
      "text": "Vamos a no sacar conclusiones precipitadas. Se está haciendo de noche, será mejor montar un campamento para pasar la noche, y ya mañana podemos pensar en todo esto."
    },
    {
      "character": "Demain",
      "portrait": "DemainTriste",
      "position": "right",
      "facingDirection": "left",
      "text": "...Vale, lo siento Lisella."
    },
    {
      "character": "Lisella",
      "portrait": "LisellaNeutral",
      "position": "left",
      "facingDirection": "right",
      "text": "No pasa nada, vamos a descansar un poco."
    }
  ]
