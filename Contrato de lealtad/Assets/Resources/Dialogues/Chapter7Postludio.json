    {
      "character": "Lisella",
      "portrait": "LisellaTriste",
      "position": "left",
      "facingDirection": "right",
      "text": "...Vale."
    },
    {
      "character": "Aldeano1V1",
      "portrait": "Aldeano1V1",
      "position": "right",
      "facingDirection": "left",
      "text": "¡Y no volváis!"
    },
    {
      "backgroundChange": "EveningHills",
      "character": "Demain",
      "portrait": "DemainTriste",
      "position": "right",
      "facingDirection": "left",
      "text": "No entiendo... ¿los mercenarios les... protegían?"
    },
    {
      "character": "Lisella",
      "portrait": "LisellaNeutral",
      "position": "left",
      "facingDirection": "right",
      "text": "Demain, respira."
    },
    {
      "character": "Demain",
      "portrait": "DemainTriste",
      "position": "right",
      "facingDirection": "left",
      "text": "Entonces... ¿para que estamos luchando? No nos necesitan."
    },
    {
      "character": "Lisella",
      "portrait": "LisellaTriste",
      "position": "left",
      "facingDirection": "right",
      "text": "..."
    },
    {
      "character": "Demain",
      "portrait": "DemainTriste",
      "position": "right",
      "facingDirection": "left",
      "text": "El reino de Hislan ya no es necesario, la gente ha evolucionado sin él."
